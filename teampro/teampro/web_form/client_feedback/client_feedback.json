 "client_script": "frappe.web_form.after_load = () => {\r\n    var child_table = frappe.web_form.get_field('product');   \r\n    var child=frappe.web_form.get_field('service');   \r\n    child_table.df.data = []; \r\n    child.df.data=[];\r\n    var rows = [\r\n        { 'aspect': 'Taste' },\r\n        { 'aspect': 'Crunchiness' },\r\n        { 'aspect': 'Freshness' },\r\n        { 'aspect': 'Packaging' },\r\n        { 'aspect': 'Product Presentation' },\r\n        { 'aspect': 'Overall Satisfaction' }\r\n    ];\r\n    rows.forEach(function(row) {\r\n        child_table.df.data.push(row);\r\n    });\r\n    child_table.refresh();\r\n    var rows1 = [\r\n        { 'aspects': 'Order Process/Experience' },\r\n        { 'aspects': 'Timeliness of Delivery' },\r\n        { 'aspects': 'Condition of Product on Arrival' },\r\n        { 'aspects': 'Customer Service' }\r\n        ];\r\n    rows1.forEach(function(row) {\r\n        child.df.data.push(row);\r\n    });\r\n    child.refresh();\r\n    // Hide 'Add Row' button for both product and service tables\r\n    const productTable = document.querySelector('[data-fieldname=\"product\"] .grid-add-row');\r\n    const serviceTable = document.querySelector('[data-fieldname=\"service\"] .grid-add-row');\r\n\r\n    if (productTable) {\r\n        productTable.style.display = 'none';\r\n    }\r\n    if (serviceTable) {\r\n        serviceTable.style.display = 'none';\r\n    }\r\n    function hideEditButtons(tableSelector) {\r\n        const editButtons = document.querySelectorAll(`${tableSelector} .grid-edit-row`);\r\n        console.log(editButtons)\r\n        editButtons.forEach(button => {\r\n            button.style.display = 'none';\r\n        });\r\n    }\r\n\r\n};\r\n",
